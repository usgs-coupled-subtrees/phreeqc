    -fixed_volume
    -pressure 1
    -volume 1
    -temperature 25
    CO2(g)    0.003
    N2(g)     0.79
    O2(g)     0.2
END
EXCHANGE 1
    X 0.001
    -equil 10
END
REACTION
	O2   1
	0.0001 mol
END
